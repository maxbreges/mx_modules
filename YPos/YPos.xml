<?xml version="1.0" encoding="UTF-8"?>
<PluginList>
    <Plugin id="YPos" name="YPos" category="mx/Sub-Controls">
        <GUI graphicsApi="GmpiGui">
            <Pin name="OnPointerDown" datatype="float" private="true"/>
            <Pin name="Value" datatype="float"/>
			<Pin name="OnPointerUp" datatype="float" private="true"/>
			<Pin name="Value (+Shift)" datatype="float" direction="out"/>
			<Pin name="Min" datatype="float" default="0"/>
			<Pin name="Max" datatype="float" default="255"/>
			<Pin name="MouseDown" datatype="bool"/>
			<Pin name="MouseDown+Shift" datatype="bool"/>
		</GUI>
    </Plugin>
</PluginList>
